#include <iostream>
#include <string>
#include <cstring>
using namespace std;

int main(void){
  bool chked[12][12];
  bool map[12][12]; //0:sea 1:land
  string str;

  while(1){
    memset(chked, false, sizeof(chked));
    for(int i=0;i<12;i++){
      getline(cin,str);
      for(int j=0;j<12;j++){
	map[i][j] = str[j] - '0';
      }
    }
  }
}
