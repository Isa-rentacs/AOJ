#include <iostream>
#include <map>
#include <algorithm>

using namespace std;

map<int, int> factor;

void decomp(int n){
  while(n%2 == 0){
    n /= 2;
    factor[2]++;
  }
  while(n%5 == 0){
    n /= 5;
    factor[5]++;
  }
}

int main(void){
  int tmp;

  while(cin >> tmp){
    factor.clear();
    decomp(tmp);
    cout << min(factor[2], factor[5]);
  }
}
