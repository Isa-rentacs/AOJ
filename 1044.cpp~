#include <iostream>
#include <string>

using namespace std;

int main(void){
  string str;
  char c;
  while(cin >> str >> c){
    int t;
    if(c == 'X')break;
    if('A' <= str[0] && str[0] <= 'Z'){
      //upper camel case
      
    }else{
      for(int i=0;i<str.size();i++){
	if(str[i] == '_'){
	  //snake case
	}else if('A' <= str[i] && str[i] <= 'Z'){
	  //lower cames case;
	}
      }
    }
  }
}
