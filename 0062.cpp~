#include <iostream>
#include <vector>

using namespace std;

int main(void){
  char tmp[10];

  while(cin >> tmp[0]){
    for(int i=1;i<10;i++){
      cin >> tmp[i+1];
    }

    for(int i=8;i>=0;i--){
      for(int j=0;j<=i;j++){
	tmp[j] = (tmp[j] + tmp[j+1]) % 10;
      }
    }
    cout << tmp[0] << endl;
  }
}
